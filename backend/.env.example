# PSA Hackathon Backend - Environment Configuration

# Application Environment
APP_ENV=dev

# Database Configuration
# Default: backend/src/app/data/database/app.db
# For testing, use ':memory:' for in-memory database
DATABASE_URL=

# Azure OpenAI API Configuration
PRIMARY_API_KEY="API_KEY_HERE"
SECONDARY_API_KEY="API_KEY_HERE"
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=1000

# Anonymous Wellbeing Mode
# Set to false to disable anonymous chat
ENABLE_ANON=true

# Development Settings
DEBUG=true
LOG_LEVEL=INFO
